</div>

<div class="col-6 mt-2">
    {{ form_label(registrationForm.ownerInformation.companyInformation.duplicateNumber) }}
    <div class="d-flex">
        <div class="form-check form-switch mt-2">
            {{ form_widget(registrationForm.ownerInformation.companyInformation.duplicateNumber , {'attr':{ 'class':'form-check-input'}}) }}
            <span class="px-1" data-toggle="tooltip" data-bs-placement="top"
                  data-bs-custom-class="custom-primary-tooltip" data-bs-title="Duplicate  Number (owner Number to company Number) ">
                                        <i class="fa-solid fa-circle-info custom_icon_color_skyblue"></i>
                                    </span>
        </div>
        {{ form_widget(registrationForm.ownerInformation.companyInformation.companyNumber) }}
    </div>
    <span class="registerError">{{ form_errors(registrationForm.ownerInformation.companyInformation.companyNumber) }}</span>
</div>

