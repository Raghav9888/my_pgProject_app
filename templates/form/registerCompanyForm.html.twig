<div class="col-md-6 mt-2">
    {{ form_label( registrationForm.ownerInformation.userNumber ) }}
    {{ form_widget(registrationForm.ownerInformation.userNumber) }}
    <span class="registerError">{{ form_errors(registrationForm.ownerInformation.userNumber) }}</span>
</div>
